{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android && npm run lint-watch",
    "lint": "esw src/**",
    "lint-watch": "esw -w --changed src/**",
    "ios": "expo start --ios && npm run lint-watch",
    "eject": "expo eject",
    "start-web": "react-scripts start",
    "build-web": "react-scripts build"
  },
  "dependencies": {
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-art": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-htmlview": "^0.14.0",
    "react-native-render-html": "^4.1.2",
    "react-native-web": "^0.11.4",
    "react-router-dom": "^5.0.0",
    "react-router-native": "^5.0.0",
    "react-scripts": "^3.0.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-airbnb": "^3.2.1",
    "babel-preset-expo": "^5.0.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-equimper": "^2.2.2",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.7.0",
    "eslint-watch": "^5.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2"
  },
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "precommit": "lint",
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "semi": false,
    "useTabs": false,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  }
}
